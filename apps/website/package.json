    "@angular/core": "19.2.3",
    "@angular/forms": "19.2.3",
    "@angular/platform-browser": "19.2.3",
    "@angular/platform-browser-dynamic": "19.2.3",
    "@angular/platform-server": "19.2.3",
    "@angular/router": "19.2.3",
    "@angular/ssr": "^19.2.5",
    "date-fns": "2.30.0",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "wavesurfer.js": "7.3.2",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.5",
