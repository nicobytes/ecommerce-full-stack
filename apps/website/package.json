{
  "name": "store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "npm run build && wrangler pages dev dist/store/cloudflare --compatibility-date=2024-05-02 --experimental-local",
    "build": "ng build && npm run process",
    "watch": "ng build --watch --configuration development",
