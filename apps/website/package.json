    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "jasmine-core": "4.6.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "postcss": "8.4.30",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.5.4",
    "typescript-eslint": "8.27.0",
