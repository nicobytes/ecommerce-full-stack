    "process": "node ./tools/copy-files.mjs && node ./tools/alter-polyfills.mjs",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.3",
    "@angular/common": "19.2.3",
